            <kind>1</kind>
            <string>///Change States
if not (obj_GUI.GameFrozen or obj_GUI.Cutscene)
{
    hSpeed = 0;
    if not (Dead or (status("PARALYZE") + 1))
    {
        if instance_exists(obj_Player)
        {
            if abs(obj_Player.x - x) &lt; (TileDimensions * 0.75)
            {
                Walking = false;
                if collision_circle(x, y, TileDimensions * 0.75, obj_Player, false, true) and not (BitePrimed or Biting)
                {
                    BitePrimed = true;
                    sprite_index = ani_Spider_primed;
                    image_index = 0;
                    image_speed = 0.3333 * obj_GUI.FrameModulus;
                }
            }
        }
        if not Biting
        {
            if Walking
            {
                hSpeed = sign(obj_Player.x - x) * 2.5;
                if abs(hSpeed) &gt; 0
                {
                    dir = sign(hSpeed);
                    sprite_index = ani_Spider_walking;
                    image_speed = 0.5 * obj_GUI.FrameModulus;
                }
            }
            else if BitePrimed
            {
                sprite_index = ani_Spider_primed;
                image_speed = 0.3333 * obj_GUI.FrameModulus;
            }
            else
            {
                sprite_index = ani_Spider;
                image_speed = 0.15 * obj_GUI.FrameModulus;
            }
        }
        else
        {
            sprite_index = ani_Spider_attacking;
            image_speed = 0.3333 * obj_GUI.FrameModulus;
        }
        image_xscale = -dir;
    }
}
else
{
    image_speed = 0;
}
