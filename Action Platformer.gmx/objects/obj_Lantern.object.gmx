            <string>///Swing the Lantern
if (not obj_GUI.GameFrozen) {
    if (not Swinging) {
        if (SwingChance != SwingThreshold) {
            SwingChance = round(random_range(1, SwingThreshold));
            if (SwingChance == SwingThreshold) {
                SetAngle = round(random_range(-20, 20));
                SwingAngle = 0;
                SwingSpeed = ceil(SwingAngle * -0.05);
                Swinging = true;
                show_debug_message("[DBG]: " + string(SetAngle) + "/" + string(SwingAngle));
            }
        }
    }
    else {
        if (abs(SwingAngle) &gt; abs(SetAngle)) {
            SwingAngle += SwingSpeed;
        }
        else {
            SetAngle = SetAngle * -0.8;
            SwingSpeed = ceil(SwingAngle * -0.05);
        }
        if (abs(SetAngle) &lt; 1) {
            SetAngle = 0;
            SwingAngle = 0;
            SwingSpeed = 0;
            Swinging = false;
            show_debug_message("[DBG]: " + string(SetAngle) + "/" + string(SwingAngle));
        }
    }
}
