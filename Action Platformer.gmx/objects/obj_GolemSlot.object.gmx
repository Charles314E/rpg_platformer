        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Push Golem out of Slot
var eDist, eDir;
if GolemZ &lt; EndingFrame
{
    if GolemZ &lt; 2
    {
        if instance_exists(obj_Tenebrane_door)
        {
            with obj_Tenebrane_door
            {
                alarm[0] = room_speed * 0.1;
            }
        }
    }
    if GolemZ = EndingFrame - 1
    {
        instance_create(x, y, obj_Golem);
        instance_create(x, y, htb_GolemSkull);
        GolemZ += 1;
        obj_GUI.Cutscene = false;
    }
    else if GolemZ &gt;= 0
    {
        GolemZ += 1;
        if GolemZ &gt;= 40
        {
            eDist = max(0, min((GolemZ - 40) * 0.15, 3));
            eDir = point_direction(x, y, obj_Player.x, obj_Player.y) + 90;
            ax = eDist * sin(eDir * pi / 180);
            ay = eDist * cos(eDir * pi / 180);
        }
        if GolemZ &gt;= 96 and GolemZ &lt;= EndingFrame - 22
        {
            if GolemZ &gt;= EndingFrame - 62
            {
                TitleAlpha -= 0.1;
                if GolemZ &gt;= EndingFrame - 42
                {
                    NameAlpha -= 0.05;
                }
            }
            else if GolemZ &gt;= 96
            {
                NameAlpha += 0.05;
                if GolemZ &gt;= 138
                {
                    TitleAlpha += 0.1;
                }
            }
            NameAlpha = min(1, max(NameAlpha, 0));
            TitleAlpha = min(1, max(TitleAlpha, 0));
        }
    }
    alarm[0] = room_speed * 0.05;
}
</string>
