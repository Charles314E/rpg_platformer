        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Initialize Lighting
if obj_Controller.MapType = "Platform"
{
    if Dark and not init
    {
        show_debug_message("[DBG]: Initializing Lighting...")
        for (yy = 0; yy &lt; room_height / TileDimensions; yy += obs)
        {
            show_debug_message("[DBG]: Lighting Progress... " + string(((yy * TileDimensions) / room_height) * 100) + "%.")
            for (xx = 0; xx &lt; room_width / TileDimensions; xx += obs)
            {
                var mx = (xx + 0.5);
                var my = (yy + 0.5);
                var rw = (room_width / TileDimensions);
                var rh = (room_height / TileDimensions);
                if place_meeting(mx * TileDimensions, my * TileDimensions, obj_Block)
                {
                    //show_debug_message("[DBG]: Block at (" + string(xx * TileDimensions) + ", " + string(yy * TileDimensions) + ").");
                    for (i = obs; i &lt; maxLightLevel + 1; i += obs)
                    {
                        blocked = true;
                        for (ii = obs; ii &lt; i + 1; ii += obs)
                        {
                            var up = place_meeting(mx * TileDimensions, (my - ii) * TileDimensions, obj_Block) or (yy &lt; i);
                            var down = place_meeting(mx * TileDimensions, (my + ii) * TileDimensions, obj_Block) or (yy &gt;= (rh - i));
                            var left = place_meeting((mx - ii) * TileDimensions, my * TileDimensions, obj_Block) or (xx &lt; i);
                            var right = place_meeting((mx + ii) * TileDimensions, my * TileDimensions, obj_Block) or (xx &gt;= (rw - i));
                            if not (up and down and left and right)
                            {
                                blocked = false;
                            }
                        }
                        if blocked
                        {
                            ds_grid_set(lightGrid, xx / obs, yy / obs, i);
                            //show_debug_message("[DBG]: Set the light level at (" + string(xx * TileDimensions) + ", " + string(yy * TileDimensions) + ") to " + string(i) + ".");
                        }
                    }
                }
            }
        }
        show_debug_message("[DBG]: Completing Lighting.")
        init = true;
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
