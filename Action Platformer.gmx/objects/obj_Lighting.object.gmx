        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw Light Levels
if obj_Controller.MapType = "Platform"
{
    var vx, vy, vw, vh, xx, yy;
    vx = view_xview;
    vy = view_yview;
    vw = view_wview;
    vh = view_hview;
    for (yy = floor(vy / (TileDimensions * obs)) * (TileDimensions * obs); yy &lt; floor((vy + vh + 16) / (TileDimensions * obs)) * (TileDimensions * obs); yy += TileDimensions * obs)
    {
        for (xx = floor(vx / (TileDimensions * obs)) * (TileDimensions * obs); xx &lt; floor((vx + vw + 16) / (TileDimensions * obs)) * (TileDimensions * obs); xx += TileDimensions * obs)
        {
            var lightLevel = ds_grid_get(lightGrid, max(0, min(xx / (TileDimensions * obs), (w / obs) - 1)), max(0, min(yy / (TileDimensions * obs), (h / obs) - 1)));
            if lightLevel &gt; 0
            {
                draw_set_halign(fa_center);
                draw_set_valign(fa_center);
                draw_set_alpha(lightLevel / maxLightLevel);
                draw_rectangle_colour(xx, yy, xx + (TileDimensions * obs) - 1, yy + (TileDimensions * obs) - 1, c_black, c_black, c_black, c_black, false);
                //draw_text_transformed(xx + (TileDimensions / 2), yy + (TileDimensions / 2), lightLevel, 0.1625, 0.1625, 0);
                draw_set_halign(fa_left);
                draw_set_valign(fa_top);
            }
        }
    }
    draw_set_alpha(1);
}
</string>
